      "title": "VictoriaMetrics data / disk %",
      "description": "Data size as percent of disk used by VictoriaMetrics (data / (data + free)). Higher values mean less free space left for data.",
      "datasource": { "type": "prometheus", "uid": "VictoriaMetrics" },
      "targets": [
        {
          "expr": "sum(vm_data_size_bytes{job=\"victoria-metrics\"}) / (sum(vm_free_disk_space_bytes{job=\"victoria-metrics\"}) + sum(vm_data_size_bytes{job=\"victoria-metrics\"}))",
          "refId": "A",
          "legendFormat": "vm data %"
        },
        {
          "expr": "sum(vm_data_size_bytes{job=\"victoria-metrics\"}) / 1024 / 1024",
          "refId": "B",
          "legendFormat": "vm data MB"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "min": 0,
          "max": 1,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 0.5 },
              { "color": "red", "value": 0.8 }
            ]
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "vm data MB" },
            "properties": [
              { "id": "unit", "value": "mbytes" },
              { "id": "min", "value": 0 },
              { "id": "max", "value": null },
              { "id": "thresholds", "value": { "mode": "absolute", "steps": [ { "color": "green", "value": null } ] } }
            ]
          }
        ]
      },
      "gridPos": { "x": 18, "y": 39, "w": 6, "h": 7 }
